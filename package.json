{
  "name": "dify-deck-slidev",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm generate && node scripts/dev.js",
    "build": "pnpm generate && pnpm -r --filter './packages/*' --filter '!theme-dify' build && node scripts/generate-index.js && npx tsx scripts/export-pdfs.js",
    "export-pdfs": "npx tsx scripts/export-pdfs.js",
    "serve": "pnpm build && npx serve dist -l 3000",
    "create-deck": "node scripts/create-deck.js",
    "generate": "npx tsx scripts/generate-slides.js",
    "generate:watch": "npx tsx watch scripts/generate-slides.js"
  },
  "devDependencies": {
    "@slidev/cli": "^52.11.0",
    "@slidev/theme-default": "latest",
    "concurrently": "^9.1.2",
    "playwright-chromium": "^1.58.1",
    "tsx": "^4.7.0"
  }
}
